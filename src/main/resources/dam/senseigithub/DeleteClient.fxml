<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dam.senseigithub.controller.clients.DeleteClientController">
    <children>
        <Label text="Borrar cliente">
            <font>
                <Font name="Cambria" size="16.0" />
            </font></Label>
        <TextField fx:id="dni" promptText="DNI" />
        <TextField fx:id="name" promptText="Nombre" />
        <Button fx:id="deleteClient" mnemonicParsing="false" onAction="#deleteClient" text="Borrar">
         <font>
            <Font name="Cambria" size="12.0" />
         </font></Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
